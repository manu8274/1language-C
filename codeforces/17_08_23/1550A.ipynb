    "t=int(input())\n",
    "for i in range(0,t):\n",
    "    sum=int(input())\n",
    "    sum1=0\n",
    "    n=1\n",
    "    l=0\n",
    "    while True:\n",
    "        a=1+(n-1)*2\n",
    "        if(sum1+a<sum):\n",
    "            sum1+=a\n",
    "            l+=1\n",
    "            n+=1\n",
    "        else:\n",
    "            print(l+1)\n",
    "            break"
