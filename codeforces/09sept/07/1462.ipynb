    "for _ in range(int(input())):\n",
    "    n=int(input())\n",
    "    lst=list(map(int,input().split()))\n",
    "    lstn=[]\n",
    "    x=n//2\n",
    "    for i in range(x):\n",
    "        lstn.append(lst[i])\n",
    "        lstn.append(lst[-i-1])\n",
    "    if(n%2!=0):\n",
    "        lstn.append(lst[x])\n",
    "    print(*(lstn))"
   ]
