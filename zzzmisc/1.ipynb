    "lst=list(map(int,input().split()))\n",
    "smax=max(lst)\n",
    "n=len(lst)\n",
    "for i in range(n):\n",
    "    for j in range(i,n):\n",
    "        sum1=sum(lst[i:j+1])\n",
    "        print(sum1)\n",
    "        if(sum1>smax):\n",
    "            smax=sum1\n",
    "print(smax)"
   ]
