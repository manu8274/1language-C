    "n=int(input())\n",
    "lst=list(map(int,input().split()))\n",
    "c=0\n",
    "for i in range(n-1):\n",
    "    x=lst[i]-lst[i+1]\n",
    "    if(x>0):\n",
    "        lst[i+1]=lst[i+1] + x\n",
    "        c+=x\n",
    "        print(i,x)\n",
    "print(c)"
