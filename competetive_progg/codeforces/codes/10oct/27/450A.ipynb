   "source": [
    "n,m=map(int,input().split())\n",
    "lst=list(map(int,input().split()))\n",
    "k=0\n",
    "c=1\n",
    "index=0\n",
    "for i in lst:\n",
    "    if(i%m>0):\n",
    "        x=i//m\n",
    "        if(x>=k):\n",
    "            index=c\n",
    "            k=x\n",
    "    else:\n",
    "        x=i//m -1\n",
    "        if(x>=k):\n",
    "            index=c\n",
    "            k=x\n",
    "    c+=1\n",
    "print(index)"
   ]
