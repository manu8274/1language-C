    "for _ in range(int(input())):\n",
    "    n=int(input())\n",
    "    lst=[abs(int(x)) for x in input().split()]\n",
    "    dc=0\n",
    "    lst.sort()\n",
    "    s=set(lst)\n",
    "    for i in s:\n",
    "        if(i==0):\n",
    "            dc+=1\n",
    "        elif(lst.count(i)>=2):\n",
    "            dc+=2\n",
    "        else:\n",
    "            dc+=1\n",
    "    print(dc)"
