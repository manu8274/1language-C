    "for _ in range(int(input())):\n",
    "    s=input()\n",
    "    l=len(s)\n",
    "    if(l%2!=0):\n",
    "        print(\"NO\")\n",
    "    else:\n",
    "        x=s[:l//2]\n",
    "        if(s.count(x)==2):\n",
    "            print(\"YES\")\n",
    "        else:\n",
    "            print(\"NO\")"
