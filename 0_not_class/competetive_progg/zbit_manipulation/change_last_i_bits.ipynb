    "def binaryprint(x):\n",
    "    y=x\n",
    "    z=\"\"\n",
    "    while(y>0):\n",
    "        z=z+str(y%2)\n",
    "        y=y//2\n",
    "    print(z[::-1])\n",
    "    \n",
    "    \n",
    "a,i=(map(int,input().split()))\n",
    "binaryprint(a)\n",
    "x=((~0)<<i)\n",
    "print(x)\n",
    "print(x&a)\n",
    "binaryprint(x&a)"
   ]
